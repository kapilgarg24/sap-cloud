PROCEDURE "SP_ADDRESS_BY_ID"(IN in_cust_id INT)
   LANGUAGE SQLSCRIPT
   SQL SECURITY INVOKER
   --DEFAULT SCHEMA <default_schema_name>
   READS SQL DATA AS
BEGIN
    SELECT ADDR.*, MKV_AT.VALUE AS ADDRESS_TYPE_TEXT FROM ADDRESS AS ADDR
    LEFT JOIN MAS_KEY_VALUE AS MKV_AT
    ON ADDR.ADDRESS_TYPE = MKV_AT.KEY AND MKV_AT.KEY_GROUP='address_type' WHERE ADDR.CUSTOMER_ID = in_cust_id;
END
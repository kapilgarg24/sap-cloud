COLUMN TABLE "PRODUCTS" (
    "PRODUCT_ID" INT PRIMARY KEY,
    "PRODUCT_DESC" nvarchar(256) NOT NULL,
    "PRODUCT_CATEGORY" NVARCHAR(8) NOT NULL,
    "PRODUCT_PRICE" FLOAT ,
    "PRODUCT_STATUS" NVARCHAR(2) NOT NULL,
    "CREATED_BY" nvarchar(64) NOT NULL,
"CREATED_AT" DATETIME NOT NULL,
"UPDATED_BY" nvarchar(64) NOT NULL,
"UPDATED_AT" DATETIME NOT NULL,
"DEL_FLAG" BOOLEAN NOT NULL
);